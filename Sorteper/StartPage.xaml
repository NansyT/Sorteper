<Page x:Class="Sorteper.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sorteper"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartPage">

    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button x:Name="startbtn" Content="Start spil" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="Startbtn_Click"/>
            <Rectangle Height="20"/>
            <Button x:Name="exitbtn" Content="Afslut spil" HorizontalAlignment="Center" VerticalAlignment="Center" Width="75" Click="Exitbtn_Click"/>
        </StackPanel>
    </Grid>
</Page>
